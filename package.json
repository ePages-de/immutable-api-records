{
  "name": "@epages/immutable-api-records",
  "description": "ImmutableJS records for the new ePages REST API.",
  "version": "0.0.66",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "build": "sh -c \"rm -rf build/ && NODE_ENV=production babel src/ --out-dir build/\"",
    "mocha": "sh -c \"NODE_ENV=test nyc mocha test/test.js\"",
    "lint": "eslint --ext js,jsx *.js src/ test/ public/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ePages-de/immutable-api-records.git"
  },
  "bugs": {
    "url": "https://github.com/ePages-de/immutable-api-records/issues"
  },
  "homepage": "https://github.com/ePages-de/immutable-api-records#readme",
  "license": "MIT",
  "main": "build/index.js",
  "peerDependencies": {
    "immutable": "^3.8.1",
    "url-template": "^2.0.8"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-plugin-istanbul": "2.0.3",
    "babel-plugin-transform-es2015-modules-umd": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "es": "0.5.2",
    "eslint": "3.9.1",
    "eslint-config-standard": "6.2.1",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-promise": "3.3.1",
    "eslint-plugin-react": "6.6.0",
    "eslint-plugin-standard": "2.0.1",
    "immutable": "3.8.1",
    "mocha": "3.1.2",
    "nyc": "8.4.0",
    "unexpected": "10.18.1",
    "url-template": "2.0.8"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
